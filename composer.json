{
  "name": "codenetix/stub-generator",
  "description": "Laravel 5 - Project stubs generator",
  "keywords": ["laravel", "stubs", "repository", "artisan", "make"],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Vorobyev",
      "email": "avorobyev@codenetix.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "avorobyev@codenetix.com"
  },
  "require": {
    "illuminate/config": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Codenetix\\StubGenerator\\": "src/Codenetix/StubGenerator/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codenetix\\StubGenerator\\StubGeneratorServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}
